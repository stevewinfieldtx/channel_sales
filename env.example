# Copy to .env locally; on Render, set these in the dashboard
OPENROUTER_API_KEY=
OPENROUTER_MODEL=anthropic/claude-3.5-sonnet
HTTP_REFERER=https://your-domain-or-render-service.onrender.com
# Render provides PORT automatically; keep fallback for local dev
PORT=3000

# Optional: enable web search integration
ENABLE_WEB_SEARCH=0
# Preferred provider: brave | serpapi (auto if key present)
SEARCH_PROVIDER=brave
BRAVE_API_KEY=
SERPAPI_API_KEY=

